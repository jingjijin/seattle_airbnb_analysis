    "parameters = {'n_estimators': [300, 400, 500],\n",
    "               'max_depth': [20, 30, 40],\n",
    "               'min_samples_split': [2, 3],\n",
    "               'min_samples_leaf': [1, 2]}\n",
    "\n",
    "grid = GridSearchCV(estimator = rf, param_grid = parameters, cv = 5)"
   ]
